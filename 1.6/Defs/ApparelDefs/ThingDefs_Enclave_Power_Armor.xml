<?xml version="1.0" encoding="utf-8"?>
<Defs>

    <!-- Enclave Advanced Power Armor -->

    <ThingDef ParentName="FCP_Apparel_Power_Armor_Base" MayRequire="Rick.FCP.PowerArmor">
        <defName>FCP_Apparel_Enclave_Advanced_Power_Armor</defName>
        <label>Enclave Advanced Power Armor</label>
        <description>A suit of advanced power armor developed by the Enclave. Offers exceptional protection against all forms of damage, enhanced mobility, and complete environmental sealing. Wearing this armor marks its user as a force to be reckoned with on the battlefield.</description>
        <generateCommonality>0.001</generateCommonality>
        <smeltable>false</smeltable>
        <thingSetMakerTags Inherit="False" />
        <possessionCount>1</possessionCount>
        <graphicData>
            <texPath>Things/Apparel/Enclave_Power_Armor/Enclave_Power_Armor/FCP_Advanced_Power_Armor</texPath>
            <graphicClass>Graphic_Single</graphicClass>
            <drawSize>1</drawSize>
        </graphicData>
        <tickerType>Normal</tickerType>
        <statBases>
            <MaxHitPoints>280</MaxHitPoints>
            <WorkToMake>12000</WorkToMake>
            <Mass>6.5</Mass>
            <MarketValue>4000</MarketValue>
            <ArmorRating_Sharp>3.60</ArmorRating_Sharp>
            <ArmorRating_Blunt>3.40</ArmorRating_Blunt>
            <ArmorRating_Heat>3.20</ArmorRating_Heat>
            <Insulation_Cold>6</Insulation_Cold>
            <Insulation_Heat>6</Insulation_Heat>
            <EquipDelay>1.1</EquipDelay>
        </statBases>
        <equippedStatOffsets>
            <MoveSpeed>0.13</MoveSpeed>
            <AimingDelayFactor>-0.07</AimingDelayFactor>
            <ToxicEnvironmentResistance>1.0</ToxicEnvironmentResistance>
            <SocialImpact>0.15</SocialImpact>
        </equippedStatOffsets>
        <apparel>
            <shellRenderedBehindHead>true</shellRenderedBehindHead>
            <bodyPartGroups>
                <li>Hands</li>
                <li>Feet</li>
                <li>Neck</li>
                <li>Legs</li>
                <li>Arms</li>
                <li>Torso</li>
                <li>Waist</li>
                <li>Shoulders</li>
            </bodyPartGroups>
            <wornGraphicPath>Things/Apparel/Enclave_Power_Armor/Enclave_Power_Armor/FCP_Advanced_Power_Armor</wornGraphicPath>
            <layers>
                <li>Shell</li>
            </layers>
            <tags>
                <li>FCP_Enclave_Power_Armor</li>
            </tags>
            <defaultOutfitTags>
                <li>Worker</li>
            </defaultOutfitTags>
            <canBeDesiredForIdeo>false</canBeDesiredForIdeo>
            <useDeflectMetalEffect>true</useDeflectMetalEffect>
            <developmentalStageFilter>Adult</developmentalStageFilter>
        </apparel>
        <tradeTags>
            <li>FCP_Power_Armor_Basic</li>
        </tradeTags>
        <building />
        <comps>
			<li Class="RangerRick_PowerArmor.CompProperties_ApparelRequirement">
				<requiredApparels>
					<li>FCP_Apparel_Power_Armor_Frame</li>
				</requiredApparels>
			</li>
		</comps>
        <modExtensions>
            <li Class="FalloutCore.ApparelExtension">
                <shouldHideBody>True</shouldHideBody>
                <displayBodyType>Hulk</displayBodyType>
            </li>
        </modExtensions>
    </ThingDef>

