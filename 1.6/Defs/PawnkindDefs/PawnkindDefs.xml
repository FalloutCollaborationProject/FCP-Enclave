    <!-- Enclave Pawnkind base -->

    <PawnKindDef Name="FCP_EnclavekindBase" Abstract="True">
		<race>Human</race>
		<defaultFactionDef>FCP_Faction_Enclave</defaultFactionDef>
		<maxGenerationAge>60</maxGenerationAge>
		<chemicalAddictionChance>0</chemicalAddictionChance>
		<invNutrition>2.7</invNutrition>
		<invFoodDef>MealSurvivalPack</invFoodDef>
		<disallowedTraits>
			<li>Nudist</li>
			<li>Brawler</li>
		</disallowedTraits>
		<requiredWorkTags>
			<li>Violent</li>
		</requiredWorkTags>
		<backstoryFiltersOverride>
			<li>
				<categoriesChildhood>
					<li>FCP_Backstory_Enclave_Child</li>
				</categoriesChildhood>
			</li>
		</backstoryFiltersOverride>
		<pawnGroupDevelopmentStage>Adult</pawnGroupDevelopmentStage>
	</PawnKindDef>

